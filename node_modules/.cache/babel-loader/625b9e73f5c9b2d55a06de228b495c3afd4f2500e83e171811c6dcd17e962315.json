{"ast":null,"code":"import roomData from \"./assets/oneroom.js\";\nimport DiscountBanner from \"./components/DiscountBanner.vue\";\nimport RoomModal from \"./components/RoomModal.vue\";\nimport RoomCard from \"./components/RoomCard.vue\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      roomData: roomData,\n      menuItems: ['Home', 'Shop', 'About'],\n      price1: 60,\n      price2: 70,\n      products: ['역삼동원룸', '천호동원룸', '마포구원룸'],\n      complaintCounts: [0, 0, 0],\n      isModalOpen: false,\n      selectedRoomNumber: 0\n    };\n  },\n  methods: {\n    increaseComplaintCount(i) {\n      this.complaintCounts[i]++;\n    },\n    openModal(id) {\n      this.selectedRoomNumber = id;\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n    }\n  },\n  components: {\n    DiscountBanner: DiscountBanner,\n    RoomModal: RoomModal,\n    RoomCard: RoomCard\n  }\n};","map":{"version":3,"names":["roomData","DiscountBanner","RoomModal","RoomCard","name","data","menuItems","price1","price2","products","complaintCounts","isModalOpen","selectedRoomNumber","methods","increaseComplaintCount","i","openModal","id","closeModal","components"],"sources":["/Users/hee/Desktop/vue/vuedongsan/src/App.vue"],"sourcesContent":["<template>\n  <RoomModal v-if=\"isModalOpen === true\" \n    :roomData=\"roomData\" \n    :roomNumber=\"selectedRoomNumber\"\n    :isModalOpen=\"isModalOpen\"\n    @closeModal=\"closeModal\"\n  />\n  <DiscountBanner/>\n  <div class=\"menu\">\n    <a v-for=\"(menu,index) in menuItems\" :key=\"index\">{{ menu }}</a>\n  </div>\n  <!-- <RoomCard  \n    v-for=\"(room,i) in roomData\" :key=\"i\"\n    :room=\"room\"\n    @openModal=\"openModal\"\n  /> -->\n  <RoomCard  \n    v-for=\"(room,i) in roomData\" :key=\"i\"\n    :room=\"room\"\n    @openModal=\"this.selectedRoomNumber = $event ; isModalOpen=true\"\n  />\n</template>\n\n<script>\nimport roomData from \"./assets/oneroom.js\"\nimport DiscountBanner from \"./components/DiscountBanner.vue\";\nimport RoomModal from \"./components/RoomModal.vue\";\nimport RoomCard from \"./components/RoomCard.vue\";\nexport default {\n  name: 'App',\n  data(){\n    return{\n      roomData : roomData,\n      menuItems: ['Home','Shop','About'],\n      price1: 60,\n      price2: 70,\n      products: ['역삼동원룸','천호동원룸','마포구원룸'],\n      complaintCounts: [0,0,0],\n      isModalOpen: false,\n      selectedRoomNumber: 0,\n    }\n  },\n  methods: {\n    increaseComplaintCount(i){\n      this.complaintCounts[i]++;\n    },\n    openModal(id){\n      this.selectedRoomNumber = id;\n      this.isModalOpen = true;\n    },\n    closeModal(){\n      this.isModalOpen = false;\n    },\n  },\n  components: {\n    DiscountBanner :DiscountBanner,\n    RoomModal :RoomModal,\n    RoomCard :RoomCard,\n},\n}\n</script>\n\n<style>\nbody{\n  margin: 0;\n}\ndiv{\n  box-sizing: border-box;\n}\n.discount-banner{\n  background: #eee;\n  padding: 10px;\n  margin: 10px;\n  border: 5px;\n}\n.black-bg{\n  width: 100%; height: 100%;\n  background: rgba(0,0,0,0.5);\n  position: fixed; padding: 20px;\n}\n.white-bg{\n  width: 100%; background: white;\n  border-radius: 8px;\n  padding: 20px;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n.menu {\n  background: darkslateblue;\n  padding: 15px;\n  border-radius: 5px;\n}\n.menu a {\n  color: white;\n  padding: 10px;\n}\n.room-img {\n  width: 100%;\n  margin-top: 40px;\n}\n</style>\n"],"mappings":"AAwBA,OAAOA,QAAO,MAAO,qBAAoB;AACzC,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJL,QAAO,EAAIA,QAAQ;MACnBM,SAAS,EAAE,CAAC,MAAM,EAAC,MAAM,EAAC,OAAO,CAAC;MAClCC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,CAAC,OAAO,EAAC,OAAO,EAAC,OAAO,CAAC;MACnCC,eAAe,EAAE,CAAC,CAAC,EAAC,CAAC,EAAC,CAAC,CAAC;MACxBC,WAAW,