{"ast":null,"code":"import roomData from \"./assets/oneroom.js\";\nimport DiscountBanner from \"./components/DiscountBanner.vue\";\nimport RoomModal from \"./components/RoomModal.vue\";\nimport RoomCard from \"./components/RoomCard.vue\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      roomData: roomData,\n      menuItems: ['Home', 'Shop', 'About'],\n      price1: 60,\n      price2: 70,\n      products: ['역삼동원룸', '천호동원룸', '마포구원룸'],\n      complaintCounts: [0, 0, 0],\n      isModalOpen: false,\n      selectedRoomNumber: 0\n    };\n  },\n  methods: {\n    test() {\n      const opCdAndStatList1 = [{\n        opCd: '060',\n        opStatCd: '001'\n      },\n      //변경 가능\n      {\n        opCd: '060',\n        opStatCd: '002'\n      },\n      //변경 불가\n      {\n        opCd: '080',\n        opStatCd: '001'\n      } //변경 불가\n      ];\n\n      const opCdAndStatList2 = [{\n        opCd: '060',\n        opStatCd: '001'\n      },\n      //변경 가능\n      {\n        opCd: '020',\n        opStatCd: '002'\n      } //변경 가능\n      ];\n\n      const opCdAndStatList4 = [{\n        opCd: '060',\n        opStatCd: '001'\n      } //변경 가능\n      ];\n\n      const opCdAndStatList3 = [{\n        opCd: '060',\n        opStatCd: '002'\n      },\n      //변경 불가\n      {\n        opCd: '080',\n        opStatCd: '001'\n      } //변경 불가\n      ];\n\n      console.log(this.checkOpCdAndStat(opCdAndStatList1));\n      console.log(this.checkOpCdAndStat(opCdAndStatList2));\n      console.log(this.checkOpCdAndStat(opCdAndStatList3));\n      console.log(this.checkOpCdAndStat(opCdAndStatList4));\n    },\n    checkOpCdAndStat(arr) {\n      // 배열을 순회하면서 조건을 확인\n      for (const item of arr) {\n        if (item.opCd !== '060' || item.opStatCd !== '001') {\n          // 하나라도 조건을 충족하지 않으면 false 반환\n          return false;\n        }\n      }\n      // 모든 객체가 조건을 충족하면 true 반환\n      return true;\n    },\n    sortDescByTitle() {\n      //문자정렬 : localeCompare\n      this.roomData.sort((a, b) => b.title.localeCompare(a.title));\n    },\n    sortAscByTitle() {\n      this.roomData.sort((a, b) => a.title.localeCompare(b.title));\n    },\n    sortDescByPrice() {\n      this.roomData.sort((a, b) => b.price - a.price);\n    },\n    sortAscByPrice() {\n      //숫자정렬 : -\n      this.roomData.sort((a, b) => {\n        return a.price - b.price;\n      });\n    },\n    increaseComplaintCount(i) {\n      this.complaintCounts[i]++;\n    },\n    openModal(id) {\n      this.selectedRoomNumber = id;\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n    }\n  },\n  components: {\n    DiscountBanner: DiscountBanner,\n    RoomModal: RoomModal,\n    RoomCard: RoomCard\n  }\n};","map":{"version":3,"names":["roomData","DiscountBanner","RoomModal","RoomCard","name","data","menuItems","price1","price2","products","complaintCounts","isModalOpen","selectedRoomNumber","methods","test","opCdAndStatList1","opCd","opStatCd","opCdAndStatList2","opCdAndStatList4","opCdAndStatList3","console","log","checkOpCdAndStat","arr","item","sortDescByTitle","sort","a","b","title","localeCompare","sortAscByTitle","sortDescByPrice","price","sortAscByPrice","increaseComplaintCount","i","openModal","id","closeModal","components"],"sources":["/Users/hee/Desktop/vue/vuedongsan/src/App.vue"],"sourcesContent":["<template>\n  <button @click=\"test\">test</button>\n  <!-- <div class=\"start\" :class=\"{end: isModalOpen}\">\n    <RoomModal\n      :roomData=\"roomData\" \n      :roomNumber=\"selectedRoomNumber\"\n      :isModalOpen=\"isModalOpen\"\n      @closeModal=\"closeModal\"\n    />\n  </div> -->\n  <Transition name=\"fade\">\n    <RoomModal \n      :roomData=\"roomData\" \n      :roomNumber=\"selectedRoomNumber\"\n      :isModalOpen=\"isModalOpen\"\n      @closeModal=\"closeModal\"\n    />\n  </Transition>\n  <DiscountBanner/>\n  <!-- 메인페이지 방문하자마자 30%라고 적힌 할인문구 1초마다 1% 감소하는거 만들기 -->\n  <button @click=\"sortAscByPrice\">sortAscByPrice</button>\n  <button @click=\"sortDescByPrice\">sortDescByPrice</button>\n  <button @click=\"sortAscByTitle\">sortAscByTitle</button>\n  <button @click=\"sortDescByTitle\">sortDescByTitle</button>\n\n  <div class=\"menu\">\n    <a v-for=\"(menu,index) in menuItems\" :key=\"index\">{{ menu }}</a>\n  </div>\n  <!-- <RoomCard  \n    v-for=\"(room,i) in roomData\" :key=\"i\"\n    :room=\"room\"\n    @openModal=\"openModal\"\n  /> -->\n  <RoomCard  \n    v-for=\"(room,i) in roomData\" :key=\"i\"\n    :room=\"room\"\n    @openModal=\"this.selectedRoomNumber = $event ; isModalOpen=true\"\n  />\n</template>\n\n<script>\nimport roomData from \"./assets/oneroom.js\"\nimport DiscountBanner from \"./components/DiscountBanner.vue\";\nimport RoomModal from \"./components/RoomModal.vue\";\nimport RoomCard from \"./components/RoomCard.vue\";\nexport default {\n  name: 'App',\n  data(){\n    return{\n      roomData : roomData,\n      menuItems: ['Home','Shop','About'],\n      price1: 60,\n      price2: 70,\n      products: ['역삼동원룸','천호동원룸','마포구원룸'],\n      complaintCounts: [0,0,0],\n      isModalOpen: false,\n      selectedRoomNumber: 0,\n    }\n  },\n  methods: {\n    test(){\n      const opCdAndStatList1 = [\n        {opCd: '060', opStatCd: '001'}, //변경 가능\n        {opCd: '060', opStatCd: '002'}, //변경 불가\n        {opCd: '080', opStatCd: '001'} //변경 불가\n      ];\n      const opCdAndStatList2 = [\n        {opCd: '060', opStatCd: '001'}, //변경 가능\n        {opCd: '020', opStatCd: '002'}, //변경 가능\n      ];\n      const opCdAndStatList4 = [\n        {opCd: '060', opStatCd: '001'}, //변경 가능\n      ];\n      const opCdAndStatList3 = [\n        {opCd: '060', opStatCd: '002'}, //변경 불가\n        {opCd: '080', opStatCd: '001'} //변경 불가\n      ];\n      console.log(this.checkOpCdAndStat(opCdAndStatList1));\n      console.log(this.checkOpCdAndStat(opCdAndStatList2));\n      console.log(this.checkOpCdAndStat(opCdAndStatList3));\n      console.log(this.checkOpCdAndStat(opCdAndStatList4));\n    },\n    checkOpCdAndStat(arr) {\n      // 배열을 순회하면서 조건을 확인\n      for (const item of arr) {\n        if (item.opCd !== '060' || item.opStatCd !== '001') {\n          // 하나라도 조건을 충족하지 않으면 false 반환\n          return false;\n        }\n      }\n      // 모든 객체가 조건을 충족하면 true 반환\n      return true;\n    },\n    sortDescByTitle(){\n      //문자정렬 : localeCompare\n      this.roomData.sort((a,b) => b.title.localeCompare(a.title))\n    },\n    sortAscByTitle(){\n      this.roomData.sort((a,b) => a.title.localeCompare(b.title))\n    },\n    sortDescByPrice(){\n      this.roomData.sort((a,b)=> b.price - a.price)\n    },\n    sortAscByPrice(){\n      //숫자정렬 : -\n      this.roomData.sort((a,b) => {\n        return a.price-b.price;\n      })\n    },\n    increaseComplaintCount(i){\n      this.complaintCounts[i]++;\n    },\n    openModal(id){\n      this.selectedRoomNumber = id;\n      this.isModalOpen = true;\n    },\n    closeModal(){\n      this.isModalOpen = false;\n    },\n  },\n  components: {\n    DiscountBanner :DiscountBanner,\n    RoomModal :RoomModal,\n    RoomCard :RoomCard,\n},\n}\n</script>\n\n<style>\n.start{\n  opacity: 0;\n  transition: all 1s;\n}\n.end{\n  opacity: 1;\n}\n.fade-enter-from{\n  /* 등장 애니메이션, 시작 스타일 */\n  opacity: 0;\n}\n.fade-enter-active{\n  /* transition */\n  transition:all 1s;\n}\n.fade-enter-to{\n  /* 등장 애니메이션, 종료 스타일 */\n  opacity: 1;\n}\n.fade-leave-from{\n  /* 퇴장 애니메이션, 시작 스타일 */\n  opacity: 1;\n}\n.fade-leave-active{\n  transition: all 1s;\n}\n.fade-leave-to{\n  /* 퇴장 애니메이션, 종료 스타일 */\n  opacity: 0;\n}\nbody{\n  margin: 0;\n}\ndiv{\n  box-sizing: border-box;\n}\n.discount-banner{\n  background: #eee;\n  padding: 10px;\n  margin: 10px;\n  border: 5px;\n}\n.black-bg{\n  width: 100%; height: 100%;\n  background: rgba(0,0,0,0.5);\n  position: fixed; padding: 20px;\n}\n.white-bg{\n  width: 100%; background: white;\n  border-radius: 8px;\n  padding: 20px;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n.menu {\n  background: darkslateblue;\n  padding: 15px;\n  border-radius: 5px;\n}\n.menu a {\n  color: white;\n  padding: 10px;\n}\n.room-img {\n  width: 100%;\n  margin-top: 40px;\n}\n</style>\n"],"mappings":"AAyCA,OAAOA,QAAO,MAAO,qBAAoB;AACzC,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJL,QAAO,EAAIA,QAAQ;MACnBM,SAAS,EAAE,CAAC,MAAM,EAAC,MAAM,EAAC,OAAO,CAAC;MAClCC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,CAAC,OAAO,EAAC,OAAO,EAAC,OAAO,CAAC;MACnCC,eAAe,EAAE,CAAC,CAAC,EAAC,CAAC,EAAC,CAAC,CAAC;MACxBC,WAAW,EAAE,KAAK;MAClBC,kBAAkB,EAAE;IACtB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,IAAIA,CAAA,EAAE;MACJ,MAAMC,gBAAe,GAAI,CACvB;QAACC,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC;MAAE;MAChC;QAACD,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC;MAAE;MAChC;QAACD,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,EAAE;MAAA,CAChC;;MACD,MAAMC,gBAAe,GAAI,CACvB;QAACF,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC;MAAE;MAChC;QAACD,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC,CAAE;MAAA,CACjC;;MACD,MAAME,gBAAe,GAAI,CACvB;QAACH,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC,CAAE;MAAA,CACjC;;MACD,MAAMG,gBAAe,GAAI,CACvB;QAACJ,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,CAAC;MAAE;MAChC;QAACD,IAAI,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAK,EAAE;MAAA,CAChC;;MACDI,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,gBAAgB,CAACR,gBAAgB,CAAC,CAAC;MACpDM,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,gBAAgB,CAACL,gBAAgB,CAAC,CAAC;MACpDG,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,gBAAgB,CAACH,gBAAgB,CAAC,CAAC;MACpDC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,gBAAgB,CAACJ,gBAAgB,CAAC,CAAC;IACtD,CAAC;IACDI,gBAAgBA,CAACC,GAAG,EAAE;MACpB;MACA,KAAK,MAAMC,IAAG,IAAKD,GAAG,EAAE;QACtB,IAAIC,IAAI,CAACT,IAAG,KAAM,KAAI,IAAKS,IAAI,CAACR,QAAO,KAAM,KAAK,EAAE;UAClD;UACA,OAAO,KAAK;QACd;MACF;MACA;MACA,OAAO,IAAI;IACb,CAAC;IACDS,eAAeA,CAAA,EAAE;MACf;MACA,IAAI,CAAC1B,QAAQ,CAAC2B,IAAI,CAAC,CAACC,CAAC,EAACC,CAAC,KAAKA,CAAC,CAACC,KAAK,CAACC,aAAa,CAACH,CAAC,CAACE,KAAK,CAAC;IA